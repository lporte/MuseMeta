<div class="body-div">
	<div class="col-md-6 col-md-offset-3" id="results">
	<h3 class="white"> Your Search Results: </h3>

		<% if @results.empty? %>
			<h3>No results found!</h3>
		<% end %>

		<div class="thumbnail image-box">
		<% @results.each do |result| %>
		  <% result.attributes.each do |attr, val| %>
		  	<% if attr.include?("img") %>
		  	  <img src="<%= val %>" >
		  	<% end %>
		  	<% unless attr.include?("password") || attr.include?("_at") %>
		  		<p >
					<dl class="dl-horizontal">
		  				<dt><%= attr %></dt>  
		  				<dd><%= val %></dd> 
		  			</dl>
		  		</p>
			<% end %>
		  <% end %>
		</div>	
		<% end %>
	

	</div>
</div>


